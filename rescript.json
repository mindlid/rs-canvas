{
  "name": "reso",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.js",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  }
}